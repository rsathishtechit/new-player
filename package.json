{
  "name": "nilaa-player",
  "productName": "Nilaa Player",
  "version": "0.0.3",
  "description": "A beautiful video course player with progress tracking",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
